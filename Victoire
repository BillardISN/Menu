from tkinter import *
import pygame

pygame.init()
pygame.mixer.init()
pygame.mixer.music.load("Victoire.wav")
pygame.mixer.music.play()

fen=Tk()
aire=Canvas(fen,height=900, width=2000)
mon_image = PhotoImage(file="GG.png", master=fen)
logo= aire.create_image(901,384, image=mon_image)
aire.image = mon_image
aire.pack()

fen.mainloop()
